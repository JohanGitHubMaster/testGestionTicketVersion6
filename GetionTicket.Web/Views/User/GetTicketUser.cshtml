@{
}
@model List<GetionTicket.Web.Models.Ticket.Ticket>

<h1>Liste de vos tickets @ViewBag.name</h1>

<table class="table">
   <thead>
      <tr>
         <th>Status</th>
         <th>Titre</th>
         <th>Déscription</th>
      </tr>
   </thead>
   <tbody>
      @if(Model!=null){
         @foreach (var ticket in Model)
         {
            <tr>
               <td>@ticket.Status</td>
               <td>@ticket.Title</td>
               <td>@ticket.Description</td>
            </tr>
         }
      }
      
   </tbody>
</table>

